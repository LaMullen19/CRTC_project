import math
import random

#dart_range is used to determine the range of how accerate the their throw is going to be
def dart_range():
    user = input("On a scale of 0 - 100, how hard do you want to throw the dart: ")
    chance = random.radiant(0, 100)
    #Need better system to have range of chance + or - 5
    rangeoff5 = (chance - 5)
    rangeon5 = (chance + 5)
    rangeoff10 = (chance - 10)
    rangeon10 = (chance + 10)
    rangeoff15 = (chance - 15)
    rangeon15 = (chance + 15)
    rangeoff20 = (chance - 20)
    rangeon20 = (chance + 20)
    rangeoff25 = (chance - 25)
    rangeon25 = (chance + 25)
    rangeoff30 = (chance - 30)
    rangeon30 = (chance + 30)
    score = []
    i = 1
    
    for i in range(3):
        print(user)
        if user > 100:
            return("Too stronk, wall will break. Lay it off a teensy bit there pal. Try again.")
        elif user < 0:
            return("How could you actually throw this backwards, there's a reason why we keep the gun cabinet locked. Try again.")
        else:
            if user == chance:
                return("CONGRADULATIONS!!! Today's your lucky day, because you automatically get access to the weapon wheel and now you get 3 shots instead of 2 for the range.")
            elif rangeoff30 < user < rangeon30:
                score.append[4]
                i += 1
            elif rangeoff25 < user < rangeon25:
                score.append[6]
                i++
            elif rangeoff20 < user < rangeon20:
                score.append[8]
                i++
            elif rangeoff15 < user < rangeon15:
                score.append[12]
                i++
            elif rangeoff10 < user < rangeon10:
                score.append[14]
                i++
            elif rangeoff5 < user < rangeon5:
                score.append[18]
                i++
            else:
                return("YOU MISSED? HAHAHA NEEEEEERD, Try again.")
    
    var sum = score[[0], + [1], + [2]]
    for append in score():
        sum()

    if score == 32:
        return("You got above the required score for the accuracy, move on to the shooting range, the weapon wheel is now accessible.")
    else:
        return("Try again, the weapon wheel is still locked.")
    
